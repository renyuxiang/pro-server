<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
      http://www.springframework.org/schema/beans/spring-beans.xsd
      http://code.alibabatech.com/schema/dubbo
      http://code.alibabatech.com/schema/dubbo/dubbo.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <dubbo:service interface="cn.win.manage.service.Manage" ref="manageService"/>
    <dubbo:service interface="cn.win.manage.service.Order" ref="orderService"/>


    <bean id="manageService" class="cn.win.manage.service.impl.ManageImpl"/>
    <bean id="orderService" class="cn.win.manage.service.impl.OrderImpl"/>
</beans>